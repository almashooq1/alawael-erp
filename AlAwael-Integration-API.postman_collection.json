                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"nationalId\": \"1234567890\",\n  \"fullName\": \"محمد علي محمد\",\n  \"dateOfBirth\": \"1990-01-15\"\n}"
                        },
                        "url": {
                            "raw": "{{base_url}}/api/integrations/government/verify-citizen",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "integrations",
                                "government",
                                "verify-citizen"
                            ]
                        }
                    }
                },
                {
                    "name": "Request Consent",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"citizenId\": \"2199123456\",\n  \"consentType\": \"health-data\",\n  \"scope\": [\"medical\", \"financial\"]\n}"
                        },
                        "url": {
                            "raw": "{{base_url}}/api/integrations/government/request-consent",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "integrations",
                                "government",
                                "request-consent"
                            ]
                        }
                    }
                },
                {
                    "name": "Get Health Records",
                    "request": {
                        "method": "GET",
                        "url": {
                            "raw": "{{base_url}}/api/integrations/government/health-records/1234567890?consentToken={{consent_token}}",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "integrations",
                                "government",
                                "health-records",
                                "1234567890"
                            ],
                            "query": [
                                {
                                    "key": "consentToken",
                                    "value": "{{consent_token}}"
                                }
                            ]
                        }
                    }
                },
                {
                    "name": "Report Incident",
                    "request": {
