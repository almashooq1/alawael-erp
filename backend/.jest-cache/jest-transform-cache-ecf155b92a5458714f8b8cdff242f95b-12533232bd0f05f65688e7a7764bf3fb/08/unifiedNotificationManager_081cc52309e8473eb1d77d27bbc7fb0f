b1ed294f8fb44ddac49d5b318557725e
/**
 * ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
 * üîî Unified Notification Manager
 * ŸÖÿØŸäÿ± ÿßŸÑÿ•ÿ¥ÿπÿßÿ±ÿßÿ™ ÿßŸÑŸÖŸàÿ≠ÿØ ÿßŸÑÿ¥ÿßŸÖŸÑ ŸàÿßŸÑÿ∞ŸÉŸä
 * ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
 * 
 * ŸÜÿ∏ÿßŸÖ ŸÖÿ™ŸÉÿßŸÖŸÑ ŸÑÿ•ÿØÿßÿ±ÿ© ÿßŸÑÿ•ÿ¥ÿπÿßÿ±ÿßÿ™ ÿπÿ®ÿ± ÿ£ŸÜŸàÿßÿ™ ŸÖÿ™ÿπÿØÿØÿ©:
 * - ÿßŸÑÿ®ÿ±ŸäÿØ ÿßŸÑÿ•ŸÑŸÉÿ™ÿ±ŸàŸÜŸä (Email)
 * - ÿ±ÿ≥ÿßÿ¶ŸÑ ŸÜÿµŸäÿ© (SMS)
 * - ÿßŸÑŸàÿßÿ™ÿ≥ ÿ¢ÿ® (WhatsApp)
 * - ÿßŸÑÿ™ŸÜÿ®ŸäŸáÿßÿ™ ÿßŸÑŸÅŸàÿ±Ÿäÿ© (In-App)
 * - ÿ•ÿ¥ÿπÿßÿ±ÿßÿ™ ÿßŸÑÿ™ÿ∑ÿ®ŸäŸÇ (Push Notifications)
 * - ŸÑŸàÿ≠ÿ© ÿßŸÑÿ™ÿ≠ŸÉŸÖ (Dashboard)
 */

const EventEmitter = require('events');
const mongoose = require('mongoose');
const nodemailer = require('nodemailer');
let twilio;
try {
  twilio = require('twilio');
} catch (e) {
  console.warn('‚ö†Ô∏è  Twilio module not installed. SMS via Twilio will be unavailable.');
}
const logger = require('../utils/logger');
const whatsappService = require('./whatsappNotificationService');

// ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
// üìã ŸÜŸÖŸàÿ∞ÿ¨ ÿßŸÑÿ•ÿ¥ÿπÿßÿ±
// ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê

const notificationSchema = new mongoose.Schema({
  // ŸÖÿπŸÑŸàŸÖÿßÿ™ ÿ£ÿ≥ÿßÿ≥Ÿäÿ©
  id: {
    type: String,
    unique: true,
    index: true
  },
  userId: {
    type: String,
    required: true,
    index: true
  },
  type: {
    type: String,
    enum: ['alert',
    // ÿ™ŸÜÿ®ŸäŸá
    'reminder',
    // ÿ™ÿ∞ŸÉŸäÿ±
    'notification',
    // ÿ•ÿ¥ÿπÿßÿ±
    'warning',
    // ÿ™ÿ≠ÿ∞Ÿäÿ±
    'critical',
    // ÿ≠ÿ±ÿ¨
    'info' // ŸÖÿπŸÑŸàŸÖÿ©
    ],
    default: 'notification'
  },
  // ÿßŸÑŸÖÿ≠ÿ™ŸàŸâ
  title: {
    en: String,
    ar: String
  },
  body: {
    en: String,
    ar: String
  },
  category: String,
  priority: {
    type: String,
    enum: ['low', 'medium', 'high', 'critical'],
    default: 'medium'
  },
  // ŸÇŸÜŸàÿßÿ™ ÿßŸÑÿ•ÿ±ÿ≥ÿßŸÑ
  channels: {
    email: {
      type: Boolean,
      default: true
    },
    sms: {
      type: Boolean,
      default: false
    },
    whatsapp: {
      type: Boolean,
      default: false
    },
    inApp: {
      type: Boolean,
      default: true
    },
    push: {
      type: Boolean,
      default: false
    },
    dashboard: {
      type: Boolean,
      default: true
    }
  },
  // ÿ≠ÿßŸÑÿ© ÿßŸÑÿ•ÿ±ÿ≥ÿßŸÑ
  status: {
    type: String,
    enum: ['draft', 'scheduled', 'sent', 'failed', 'read', 'archived'],
    default: 'draft',
    index: true
  },
  // ŸÖÿπŸÑŸàŸÖÿßÿ™ ÿßŸÑÿ¨Ÿáÿßÿ™ ÿßŸÑŸÖÿ™ÿπŸÑŸÇÿ©
  relatedData: {
    entityType: String,
    // ŸÜŸàÿπ ÿßŸÑŸÉÿßÿ¶ŸÜ ÿßŸÑŸÖÿ±ÿ™ÿ®ÿ∑
    entityId: String,
    // ŸÖÿπÿ±ŸÅ ÿßŸÑŸÉÿßÿ¶ŸÜ
    actionUrl: String // ÿ±ÿßÿ®ÿ∑ ÿßŸÑÿ•ÿ¨ÿ±ÿßÿ°
  },
  // ÿßŸÑÿ™Ÿàÿßÿ±ŸäÿÆ
  createdAt: {
    type: Date,
    default: Date.now,
    index: true
  },
  scheduledAt: Date,
  sentAt: Date,
  readAt: Date,
  expiresAt: Date,
  // ÿßŸÑÿ™ÿ™ÿ®ÿπ
  deliveryStatus: {
    email: {
      sent: Boolean,
      sentAt: Date,
      error: String
    },
    sms: {
      sent: Boolean,
      sentAt: Date,
      messageId: String,
      error: String
    },
    whatsapp: {
      sent: Boolean,
      sentAt: Date,
      messageId: String,
      error: String
    },
    inApp: {
      sent: Boolean,
      sentAt: Date
    },
    push: {
      sent: Boolean,
      sentAt: Date,
      error: String
    },
    dashboard: {
      sent: Boolean,
      sentAt: Date
    }
  },
  // ÿßŸÑÿ®ŸäÿßŸÜÿßÿ™ ÿßŸÑÿ•ÿ∂ÿßŸÅŸäÿ©
  metadata: mongoose.Schema.Types.Mixed,
  tags: [String],
  // ÿßŸÑÿ£ÿØÿßÿ°
  rate: Number,
  // ÿ™ŸÇŸäŸäŸÖ ÿßŸÑŸÖÿ≥ÿ™ÿÆÿØŸÖ (1-5)
  ratedAt: Date,
  feedback: String
});

// ÿ•ŸÜÿ¥ÿßÿ° ŸÅŸáÿ±ÿ≥ÿ© ŸÖÿ±ŸÉÿ®ÿ© ŸÑÿ™ÿ≠ÿ≥ŸäŸÜ ÿßŸÑÿ£ÿØÿßÿ°
notificationSchema.index({
  userId: 1,
  createdAt: -1
});
notificationSchema.index({
  userId: 1,
  status: 1
});
notificationSchema.index({
  userId: 1,
  category: 1
});
const Notification = mongoose.model('Notification', notificationSchema);

// ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
// üéØ ŸÖÿØŸäÿ± ÿßŸÑÿ•ÿ¥ÿπÿßÿ±ÿßÿ™ ÿßŸÑŸÖŸàÿ≠ÿØ
// ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê

class UnifiedNotificationManager extends EventEmitter {
  constructor(config = {}) {
    super();

    // ÿßŸÑÿ•ÿπÿØÿßÿØÿßÿ™
    this.config = {
      maxRetries: 3,
      retryDelay: 5000,
      batchSize: 50,
      ...config
    };

    // ÿßŸÑÿ®ÿ±ŸäÿØ ÿßŸÑÿ•ŸÑŸÉÿ™ÿ±ŸàŸÜŸä
    this.emailTransporter = nodemailer.createTransport({
      service: process.env.EMAIL_SERVICE || 'gmail',
      auth: {
        user: process.env.EMAIL_USER,
        pass: process.env.EMAIL_PASSWORD
      }
    });

    // Twilio ŸÑŸÑÿ±ÿ≥ÿßÿ¶ŸÑ ÿßŸÑŸÜÿµŸäÿ©
    this.twilioClient = null;
    if (twilio && process.env.TWILIO_ACCOUNT_SID && process.env.TWILIO_AUTH_TOKEN) {
      this.twilioClient = twilio(process.env.TWILIO_ACCOUNT_SID, process.env.TWILIO_AUTH_TOKEN);
    }

    // ŸÇÿßÿ¶ŸÖÿ© ÿßŸÑÿßŸÜÿ™ÿ∏ÿßÿ±
    this.queue = [];
    this.isProcessing = false;

    // ÿßŸÑÿ•ÿ≠ÿµÿßÿ¶Ÿäÿßÿ™
    this.stats = {
      total: 0,
      sent: 0,
      failed: 0,
      channelStats: {
        email: {
          sent: 0,
          failed: 0
        },
        sms: {
          sent: 0,
          failed: 0
        },
        whatsapp: {
          sent: 0,
          failed: 0
        },
        inApp: {
          sent: 0,
          failed: 0
        },
        push: {
          sent: 0,
          failed: 0
        },
        dashboard: {
          sent: 0,
          failed: 0
        }
      }
    };
    this.startQueueProcessor();
  }

  /**
   * ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
   * üì§ ÿ•ÿ±ÿ≥ÿßŸÑ ÿßŸÑÿ•ÿ¥ÿπÿßÿ±ÿßÿ™
   * ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
   */

  /**
   * ÿ•ÿ±ÿ≥ÿßŸÑ ÿ•ÿ¥ÿπÿßÿ± ŸÖŸàÿ≠ÿØ
   */
  async sendNotification(userId, notificationData) {
    try {
      // ÿßŸÑÿ™ÿ≠ŸÇŸÇ ŸÖŸÜ ÿßŸÑŸÖÿØÿÆŸÑÿßÿ™
      if (!userId || !notificationData) {
        throw new Error('ŸÖÿπŸÑŸàŸÖÿßÿ™ ÿßŸÑÿ•ÿ¥ÿπÿßÿ± ÿ∫Ÿäÿ± ŸÉÿßŸÖŸÑÿ©');
      }

      // ÿ•ŸÜÿ¥ÿßÿ° ÿßŸÑÿ•ÿ¥ÿπÿßÿ±
      const notification = new Notification({
        id: `notif_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`,
        userId,
        ...notificationData
      });

      // ÿ≠ŸÅÿ∏ ŸÅŸä ŸÇÿßÿπÿØÿ© ÿßŸÑÿ®ŸäÿßŸÜÿßÿ™
      const savedNotification = await notification.save();

      // ÿ•ÿ∂ÿßŸÅÿ© ÿ•ŸÑŸâ ŸÇÿßÿ¶ŸÖÿ© ÿßŸÑÿßŸÜÿ™ÿ∏ÿßÿ±
      this.queue.push({
        notification: savedNotification,
        retries: 0
      });

      // ÿ•ÿ∑ŸÑÿßŸÇ ÿ≠ÿØÿ´
      this.emit('notificationCreated', savedNotification);
      logger.info(`üì¨ ÿ•ÿ¥ÿπÿßÿ± ŸÖŸàÿ≠ÿØ ŸÖÿ∂ÿßŸÅ ÿ•ŸÑŸâ ŸÇÿßÿ¶ŸÖÿ© ÿßŸÑÿßŸÜÿ™ÿ∏ÿßÿ±: ${userId}`);
      return savedNotification;
    } catch (error) {
      logger.error(`‚ùå ÿÆÿ∑ÿ£ ŸÅŸä ÿ•ÿ±ÿ≥ÿßŸÑ ÿßŸÑÿ•ÿ¥ÿπÿßÿ±: ${error.message}`);
      this.emit('notificationError', {
        userId,
        error: error.message
      });
      throw error;
    }
  }

  /**
   * ÿ•ÿ±ÿ≥ÿßŸÑ ÿ•ÿ¥ÿπÿßÿ± ŸÅŸàÿ±Ÿä (ÿ≠ÿßŸÑŸä)
   */
  async sendImmediateNotification(userId, notificationData) {
    try {
      const notification = new Notification({
        id: `insta_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`,
        userId,
        status: 'sent',
        ...notificationData
      });
      const savedNotification = await notification.save();

      // ŸÖÿ≠ÿßŸàŸÑÿ© ÿßŸÑÿ•ÿ±ÿ≥ÿßŸÑ ÿßŸÑŸÅŸàÿ±Ÿä ÿπÿ®ÿ± ÿ¨ŸÖŸäÿπ ÿßŸÑŸÇŸÜŸàÿßÿ™
      await this.deliverNotification(savedNotification);
      this.emit('immediateNotificationSent', savedNotification);
      return savedNotification;
    } catch (error) {
      logger.error(`‚ùå ÿÆÿ∑ÿ£ ŸÅŸä ÿßŸÑÿ•ÿ±ÿ≥ÿßŸÑ ÿßŸÑŸÅŸàÿ±Ÿä: ${error.message}`);
      throw error;
    }
  }

  /**
   * ÿ•ÿ±ÿ≥ÿßŸÑ ÿ•ÿ¥ÿπÿßÿ±ÿßÿ™ ÿ¨ŸÖÿßÿπŸäÿ©
   */
  async sendBulkNotifications(userIds, notificationTemplate) {
    try {
      const results = [];
      for (const userId of userIds) {
        try {
          const result = await this.sendNotification(userId, notificationTemplate);
          results.push({
            userId,
            status: 'queued',
            id: result.id
          });
        } catch (error) {
          results.push({
            userId,
            status: 'failed',
            error: error.message
          });
        }
      }
      logger.info(`üì¨ ${results.filter(r => r.status === 'queued').length} ÿ•ÿ¥ÿπÿßÿ± ÿ¨ŸÖÿßÿπŸä ÿ£ÿ∂ŸäŸÅ ÿ•ŸÑŸâ ŸÇÿßÿ¶ŸÖÿ© ÿßŸÑÿßŸÜÿ™ÿ∏ÿßÿ±`);
      return results;
    } catch (error) {
      logger.error(`‚ùå ÿÆÿ∑ÿ£ ŸÅŸä ÿßŸÑÿ•ÿ±ÿ≥ÿßŸÑ ÿßŸÑÿ¨ŸÖÿßÿπŸä: ${error.message}`);
      throw error;
    }
  }

  /**
   * ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
   * üöö ÿ™ÿ≥ŸÑŸäŸÖ ÿßŸÑÿ•ÿ¥ÿπÿßÿ±ÿßÿ™
   * ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
   */

  /**
   * ŸÖÿπÿßŸÑÿ¨ ŸÇÿßÿ¶ŸÖÿ© ÿßŸÑÿßŸÜÿ™ÿ∏ÿßÿ±
   */
  startQueueProcessor() {
    setInterval(async () => {
      if (!this.isProcessing && this.queue.length > 0) {
        await this.processQueue();
      }
    }, 1000);
    logger.info('‚ñ∂Ô∏è ŸÖÿπÿßŸÑÿ¨ ŸÇÿßÿ¶ŸÖÿ© ÿßŸÜÿ™ÿ∏ÿßÿ± ÿßŸÑÿ•ÿ¥ÿπÿßÿ±ÿßÿ™ ŸÜÿ¥ÿ∑');
  }

  /**
   * ŸÖÿπÿßŸÑÿ¨ÿ© ŸÇÿßÿ¶ŸÖÿ© ÿßŸÑÿßŸÜÿ™ÿ∏ÿßÿ±
   */
  async processQueue() {
    if (this.isProcessing || this.queue.length === 0) {
      return;
    }
    this.isProcessing = true;
    try {
      const batch = this.queue.splice(0, this.config.batchSize);
      for (const item of batch) {
        try {
          await this.deliverNotification(item.notification);
        } catch (error) {
          item.retries++;
          if (item.retries < this.config.maxRetries) {
            // ÿ•ÿπÿßÿØÿ© ŸÖÿ≠ÿßŸàŸÑÿ©
            setTimeout(() => this.queue.push(item), this.config.retryDelay * item.retries);
          } else {
            // ŸÅÿ¥ŸÑ ŸÜŸáÿßÿ¶Ÿä
            await Notification.updateOne({
              id: item.notification.id
            }, {
              status: 'failed'
            });
            logger.error(`‚ùå ŸÅÿ¥ŸÑ ÿßŸÑÿ•ÿ¥ÿπÿßÿ± ÿ®ÿπÿØ ${item.retries} ŸÖÿ≠ÿßŸàŸÑÿßÿ™: ${item.notification.id}`);
          }
        }
      }
    } finally {
      this.isProcessing = false;
    }
  }

  /**
   * ÿ™ÿ≥ŸÑŸäŸÖ ÿßŸÑÿ•ÿ¥ÿπÿßÿ± ÿπÿ®ÿ± ÿßŸÑŸÇŸÜŸàÿßÿ™ ÿßŸÑŸÖÿÆÿ™ÿßÿ±ÿ©
   */
  async deliverNotification(notification) {
    const deliveryPromises = [];

    // ÿßŸÑÿ®ÿ±ŸäÿØ ÿßŸÑÿ•ŸÑŸÉÿ™ÿ±ŸàŸÜŸä
    if (notification.channels.email) {
      deliveryPromises.push(this.sendEmailNotification(notification).catch(err => ({
        channel: 'email',
        error: err
      })));
    }

    // ÿßŸÑŸàÿßÿ™ÿ≥ ÿ¢ÿ®
    if (notification.channels.whatsapp) {
      deliveryPromises.push(this.sendWhatsAppNotification(notification).catch(err => ({
        channel: 'whatsapp',
        error: err
      })));
    }

    // ÿßŸÑÿ±ÿ≥ÿßÿ¶ŸÑ ÿßŸÑŸÜÿµŸäÿ©
    if (notification.channels.sms) {
      deliveryPromises.push(this.sendSMSNotification(notification).catch(err => ({
        channel: 'sms',
        error: err
      })));
    }

    // ÿßŸÑÿ•ÿ¥ÿπÿßÿ±ÿßÿ™ ÿßŸÑŸÅŸàÿ±Ÿäÿ©
    if (notification.channels.inApp) {
      deliveryPromises.push(this.sendInAppNotification(notification).catch(err => ({
        channel: 'inApp',
        error: err
      })));
    }

    // ÿ•ÿ¥ÿπÿßÿ±ÿßÿ™ ÿßŸÑÿ™ÿ∑ÿ®ŸäŸÇ
    if (notification.channels.push) {
      deliveryPromises.push(this.sendPushNotification(notification).catch(err => ({
        channel: 'push',
        error: err
      })));
    }

    // ŸÑŸàÿ≠ÿ© ÿßŸÑÿ™ÿ≠ŸÉŸÖ
    if (notification.channels.dashboard) {
      deliveryPromises.push(this.sendDashboardNotification(notification).catch(err => ({
        channel: 'dashboard',
        error: err
      })));
    }

    // ÿßŸÜÿ™ÿ∏ÿßÿ± ÿ¨ŸÖŸäÿπ ÿßŸÑÿ•ÿ±ÿ≥ÿßŸÑÿßÿ™
    const results = await Promise.allSettled(deliveryPromises);

    // ÿ™ÿ≠ÿØŸäÿ´ ÿ≠ÿßŸÑÿ© ÿßŸÑÿ•ÿ¥ÿπÿßÿ±
    notification.status = 'sent';
    notification.sentAt = new Date();
    await notification.save();
    this.emit('notificationDelivered', notification);
    logger.info(`‚úÖ ÿ™ŸÖ ÿ™ÿ≥ŸÑŸäŸÖ ÿßŸÑÿ•ÿ¥ÿπÿßÿ±: ${notification.id}`);
  }

  /**
   * ÿ•ÿ±ÿ≥ÿßŸÑ ÿπÿ®ÿ± ÿßŸÑÿ®ÿ±ŸäÿØ ÿßŸÑÿ•ŸÑŸÉÿ™ÿ±ŸàŸÜŸä
   */
  async sendEmailNotification(notification) {
    try {
      // ÿßŸÑÿ≠ÿµŸàŸÑ ÿπŸÑŸâ ÿ®ŸäÿßŸÜÿßÿ™ ÿßŸÑŸÖÿ≥ÿ™ÿÆÿØŸÖ (Ÿäÿ¨ÿ® ÿßÿ≥ÿ™ÿØÿπÿßÿ° ŸÇÿßÿπÿØÿ© ÿßŸÑÿ®ŸäÿßŸÜÿßÿ™)
      // const user = await User.findById(notification.userId);

      const htmlContent = this.buildEmailTemplate(notification);
      const mailOptions = {
        from: process.env.EMAIL_USER,
        to: notification.metadata?.email || 'user@example.com',
        subject: notification.title?.ar || notification.title?.en,
        html: htmlContent
      };
      await this.emailTransporter.sendMail(mailOptions);

      // ÿ™ÿ≠ÿØŸäÿ´ ÿ≠ÿßŸÑÿ© ÿßŸÑÿ®ÿ±ŸäÿØ ÿßŸÑÿ•ŸÑŸÉÿ™ÿ±ŸàŸÜŸä
      await Notification.updateOne({
        id: notification.id
      }, {
        'deliveryStatus.email.sent': true,
        'deliveryStatus.email.sentAt': new Date()
      });
      this.stats.channelStats.email.sent++;
      logger.info(`üìß ÿ®ÿ±ŸäÿØ ÿ•ŸÑŸÉÿ™ÿ±ŸàŸÜŸä ŸÖÿ±ÿ≥ŸÑ: ${notification.userId}`);
    } catch (error) {
      this.stats.channelStats.email.failed++;
      logger.error(`‚ùå ÿÆÿ∑ÿ£ ŸÅŸä ÿ•ÿ±ÿ≥ÿßŸÑ ÿßŸÑÿ®ÿ±ŸäÿØ ÿßŸÑÿ•ŸÑŸÉÿ™ÿ±ŸàŸÜŸä: ${error.message}`);
      throw error;
    }
  }

  /**
   * ÿ•ÿ±ÿ≥ÿßŸÑ ÿπÿ®ÿ± ÿßŸÑŸàÿßÿ™ÿ≥ ÿ¢ÿ®
   */
  async sendWhatsAppNotification(notification) {
    try {
      const phoneNumber = notification.metadata?.phoneNumber;
      if (!phoneNumber) {
        throw new Error('ÿ±ŸÇŸÖ ÿßŸÑŸáÿßÿ™ŸÅ ÿ∫Ÿäÿ± ŸÖÿ™ŸàŸÅÿ±');
      }
      const message = this.buildWhatsAppMessage(notification);
      await whatsappService.sendMessage(phoneNumber, message);

      // ÿ™ÿ≠ÿØŸäÿ´ ÿ≠ÿßŸÑÿ© ÿßŸÑŸàÿßÿ™ÿ≥ ÿ¢ÿ®
      await Notification.updateOne({
        id: notification.id
      }, {
        'deliveryStatus.whatsapp.sent': true,
        'deliveryStatus.whatsapp.sentAt': new Date()
      });
      this.stats.channelStats.whatsapp.sent++;
      logger.info(`üí¨ ÿ±ÿ≥ÿßŸÑÿ© Ÿàÿßÿ™ÿ≥ ÿ¢ÿ® ŸÖÿ±ÿ≥ŸÑÿ©: ${phoneNumber}`);
    } catch (error) {
      this.stats.channelStats.whatsapp.failed++;
      logger.error(`‚ùå ÿÆÿ∑ÿ£ ŸÅŸä ÿ•ÿ±ÿ≥ÿßŸÑ ÿßŸÑŸàÿßÿ™ÿ≥ ÿ¢ÿ®: ${error.message}`);
      throw error;
    }
  }

  /**
   * ÿ•ÿ±ÿ≥ÿßŸÑ ÿ±ÿ≥ÿßŸÑÿ© ŸÜÿµŸäÿ©
   */
  async sendSMSNotification(notification) {
    try {
      // ÿ™ÿ≠ŸÇŸÇ ŸÖŸÜ ÿ™ŸàŸÅÿ± Twilio
      if (!this.twilioClient) {
        logger.warn('‚ö†Ô∏è Twilio client not configured. SMS notification skipped.');
        this.stats.channelStats.sms.skipped = (this.stats.channelStats.sms.skipped || 0) + 1;
        return {
          success: false,
          message: 'Twilio not configured'
        };
      }
      const phoneNumber = notification.metadata?.phoneNumber;
      if (!phoneNumber) {
        throw new Error('ÿ±ŸÇŸÖ ÿßŸÑŸáÿßÿ™ŸÅ ÿ∫Ÿäÿ± ŸÖÿ™ŸàŸÅÿ±');
      }
      const message = (notification.body?.ar || notification.body?.en || '').substring(0, 160);
      const smsMessage = await this.twilioClient.messages.create({
        body: message,
        from: process.env.TWILIO_PHONE_NUMBER,
        to: phoneNumber
      });

      // ÿ™ÿ≠ÿØŸäÿ´ ÿ≠ÿßŸÑÿ© ÿßŸÑÿ±ÿ≥ÿßŸÑÿ© ÿßŸÑŸÜÿµŸäÿ©
      await Notification.updateOne({
        id: notification.id
      }, {
        'deliveryStatus.sms.sent': true,
        'deliveryStatus.sms.sentAt': new Date(),
        'deliveryStatus.sms.messageId': smsMessage.sid
      });
      this.stats.channelStats.sms.sent++;
      logger.info(`üì± ÿ±ÿ≥ÿßŸÑÿ© ŸÜÿµŸäÿ© ŸÖÿ±ÿ≥ŸÑÿ©: ${phoneNumber}`);
    } catch (error) {
      this.stats.channelStats.sms.failed++;
      logger.error(`‚ùå ÿÆÿ∑ÿ£ ŸÅŸä ÿ•ÿ±ÿ≥ÿßŸÑ ÿßŸÑÿ±ÿ≥ÿßŸÑÿ© ÿßŸÑŸÜÿµŸäÿ©: ${error.message}`);
      throw error;
    }
  }

  /**
   * ÿ•ÿ±ÿ≥ÿßŸÑ ÿ•ÿ¥ÿπÿßÿ± ŸÅŸàÿ±Ÿä
   */
  async sendInAppNotification(notification) {
    try {
      // ÿßŸÑÿ•ÿ¥ÿπÿßÿ± ÿßŸÑŸÅŸàÿ±Ÿä ŸäŸèÿÆÿ≤ŸÜ ŸÅŸä ŸÇÿßÿπÿØÿ© ÿßŸÑÿ®ŸäÿßŸÜÿßÿ™ ŸàŸäŸèÿπÿ±ÿ∂ ŸÖÿ®ÿßÿ¥ÿ±ÿ© ŸÑŸÑŸÖÿ≥ÿ™ÿÆÿØŸÖ
      // ŸÅŸä ÿßŸÑŸàÿßÿ¨Ÿáÿ© ÿßŸÑÿ£ŸÖÿßŸÖŸäÿ© ÿπŸÜÿØ ÿßŸÑÿßÿ™ÿµÿßŸÑ ÿ®ŸÄ API

      await Notification.updateOne({
        id: notification.id
      }, {
        'deliveryStatus.inApp.sent': true,
        'deliveryStatus.inApp.sentAt': new Date()
      });
      this.stats.channelStats.inApp.sent++;
      logger.info(`üîî ÿ•ÿ¥ÿπÿßÿ± ŸÅŸàÿ±Ÿä ŸÑŸÑŸÖÿ≥ÿ™ÿÆÿØŸÖ: ${notification.userId}`);

      // ÿ®ÿ´ ÿßŸÑÿ•ÿ¥ÿπÿßÿ± ÿπÿ®ÿ± WebSocket (ÿßÿÆÿ™Ÿäÿßÿ±Ÿä)
      this.emit('inAppNotification', {
        userId: notification.userId,
        notification
      });
    } catch (error) {
      this.stats.channelStats.inApp.failed++;
      throw error;
    }
  }

  /**
   * ÿ•ÿ±ÿ≥ÿßŸÑ ÿ•ÿ¥ÿπÿßÿ± ÿßŸÑÿ™ÿ∑ÿ®ŸäŸÇ
   */
  async sendPushNotification(notification) {
    try {
      // ÿ™ŸÜŸÅŸäÿ∞ ŸÖÿπ Firebase Cloud Messaging ÿ£Ÿà ÿÆÿØŸÖÿ© ÿ£ÿÆÿ±Ÿâ
      // ŸáŸÜÿß ŸÖÿ´ÿßŸÑ ÿπŸÑŸâ ÿßŸÑŸáŸäŸÉŸÑ ŸÅŸÇÿ∑

      await Notification.updateOne({
        id: notification.id
      }, {
        'deliveryStatus.push.sent': true,
        'deliveryStatus.push.sentAt': new Date()
      });
      this.stats.channelStats.push.sent++;
      logger.info(`üì≤ ÿ•ÿ¥ÿπÿßÿ± ÿ™ÿ∑ÿ®ŸäŸÇ ŸÖÿ±ÿ≥ŸÑ: ${notification.userId}`);
    } catch (error) {
      this.stats.channelStats.push.failed++;
      throw error;
    }
  }

  /**
   * ÿ•ÿ±ÿ≥ÿßŸÑ ÿ•ŸÑŸâ ŸÑŸàÿ≠ÿ© ÿßŸÑÿ™ÿ≠ŸÉŸÖ
   */
  async sendDashboardNotification(notification) {
    try {
      await Notification.updateOne({
        id: notification.id
      }, {
        'deliveryStatus.dashboard.sent': true,
        'deliveryStatus.dashboard.sentAt': new Date()
      });
      this.stats.channelStats.dashboard.sent++;

      // ÿ®ÿ´ ÿ•ŸÑŸâ ŸÑŸàÿ≠ÿ© ÿßŸÑÿ™ÿ≠ŸÉŸÖ ÿπÿ®ÿ± WebSocket
      this.emit('dashboardNotification', {
        userId: notification.userId,
        notification
      });
      logger.info(`üìä ÿ•ÿ¥ÿπÿßÿ± ŸÑŸàÿ≠ÿ© ÿßŸÑÿ™ÿ≠ŸÉŸÖ: ${notification.userId}`);
    } catch (error) {
      this.stats.channelStats.dashboard.failed++;
      throw error;
    }
  }

  /**
   * ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
   * üì® ÿ®ŸÜÿßÿ° ŸÇŸàÿßŸÑÿ® ÿßŸÑÿ±ÿ≥ÿßÿ¶ŸÑ
   * ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
   */

  /**
   * ÿ®ŸÜÿßÿ° ŸÇÿßŸÑÿ® ÿßŸÑÿ®ÿ±ŸäÿØ ÿßŸÑÿ•ŸÑŸÉÿ™ÿ±ŸàŸÜŸä
   */
  buildEmailTemplate(notification) {
    const lang = notification.metadata?.language || 'ar';
    const title = notification.title?.[lang] || notification.title?.en || '';
    const body = notification.body?.[lang] || notification.body?.en || '';
    const isArabic = lang === 'ar';
    const direction = isArabic ? 'rtl' : 'ltr';
    return `
      <!DOCTYPE html>
      <html dir="${direction}">
      <head>
        <meta charset="UTF-8">
        <style>
          body { font-family: Arial, sans-serif; margin: 0; padding: 0; }
          .container { max-width: 600px; margin: 0 auto; padding: 20px; }
          .header { background-color: #007bff; color: white; padding: 20px; border-radius: 5px 5px 0 0; }
          .content { background-color: #f9f9f9; padding: 20px; }
          .footer { background-color: #e9ecef; padding: 10px; border-radius: 0 0 5px 5px; font-size: 12px; }
          .button { display: inline-block; background-color: #007bff; color: white; padding: 10px 20px; text-decoration: none; border-radius: 5px; margin-top: 10px; }
        </style>
      </head>
      <body>
        <div class="container">
          <div class="header">
            <h2>${title}</h2>
          </div>
          <div class="content">
            <p>${body}</p>
            ${notification.relatedData?.actionUrl ? `
              <a href="${notification.relatedData.actionUrl}" class="button">
                ${isArabic ? 'ÿπÿ±ÿ∂ ÿßŸÑÿ™ŸÅÿßÿµŸäŸÑ' : 'View Details'}
              </a>
            ` : ''}
          </div>
          <div class="footer">
            <p>${isArabic ? 'Ÿáÿ∞ÿß ÿ®ÿ±ŸäÿØ ÿ™ŸÑŸÇÿßÿ¶Ÿäÿå Ÿäÿ±ÿ¨Ÿâ ÿπÿØŸÖ ÿßŸÑÿ±ÿØ ÿπŸÑŸäŸá' : 'This is an automated email, please do not reply'}</p>
          </div>
        </div>
      </body>
      </html>
    `;
  }

  /**
   * ÿ®ŸÜÿßÿ° ÿ±ÿ≥ÿßŸÑÿ© ÿßŸÑŸàÿßÿ™ÿ≥ ÿ¢ÿ®
   */
  buildWhatsAppMessage(notification) {
    const lang = notification.metadata?.language || 'ar';
    const title = notification.title?.[lang] || notification.title?.en || '';
    const body = notification.body?.[lang] || notification.body?.en || '';
    const prefix = this.getNotificationEmoji(notification.type);
    return `${prefix} *${title}*\n\n${body}`;
  }

  /**
   * ÿßŸÑÿ≠ÿµŸàŸÑ ÿπŸÑŸâ ÿßŸÑÿ±ŸÖŸàÿ≤ ÿßŸÑÿ™ÿπÿ®Ÿäÿ±Ÿäÿ©
   */
  getNotificationEmoji(type) {
    const emojis = {
      alert: 'üö®',
      reminder: '‚è∞',
      notification: 'üì¢',
      warning: '‚ö†Ô∏è',
      critical: 'üî¥',
      info: '‚ÑπÔ∏è'
    };
    return emojis[type] || 'üì¢';
  }

  /**
   * ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
   * üìä ÿßŸÑÿ•ÿ≠ÿµÿßÿ¶Ÿäÿßÿ™ ŸàÿßŸÑÿ™ŸÇÿßÿ±Ÿäÿ±
   * ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
   */

  /**
   * ÿßŸÑÿ≠ÿµŸàŸÑ ÿπŸÑŸâ ÿßŸÑÿ•ÿ≠ÿµÿßÿ¶Ÿäÿßÿ™
   */
  getStatistics() {
    return {
      total: this.stats.total,
      sent: this.stats.sent,
      failed: this.stats.failed,
      successRate: this.stats.total > 0 ? (this.stats.sent / this.stats.total * 100).toFixed(2) + '%' : '0%',
      channelStats: this.stats.channelStats,
      queueLength: this.queue.length,
      isProcessing: this.isProcessing
    };
  }

  /**
   * ÿßŸÑÿ≠ÿµŸàŸÑ ÿπŸÑŸâ ÿ•ÿ¥ÿπÿßÿ±ÿßÿ™ ÿßŸÑŸÖÿ≥ÿ™ÿÆÿØŸÖ
   */
  async getUserNotifications(userId, filters = {}) {
    try {
      const query = {
        userId
      };
      if (filters.status) {
        query.status = filters.status;
      }
      if (filters.type) {
        query.type = filters.type;
      }
      if (filters.category) {
        query.category = filters.category;
      }
      const notifications = await Notification.find(query).sort({
        createdAt: -1
      }).limit(filters.limit || 50).exec();
      return notifications;
    } catch (error) {
      logger.error(`‚ùå ÿÆÿ∑ÿ£ ŸÅŸä ÿ¨ŸÑÿ® ÿßŸÑÿ•ÿ¥ÿπÿßÿ±ÿßÿ™: ${error.message}`);
      throw error;
    }
  }

  /**
   * Ÿàÿ∂ÿπ ÿπŸÑÿßŸÖÿ© ÿπŸÑŸâ ÿßŸÑÿ•ÿ¥ÿπÿßÿ± ŸÉŸÖŸÇÿ±Ÿàÿ°
   */
  async markAsRead(notificationId) {
    try {
      const result = await Notification.updateOne({
        id: notificationId
      }, {
        status: 'read',
        readAt: new Date()
      });
      return result;
    } catch (error) {
      logger.error(`‚ùå ÿÆÿ∑ÿ£ ŸÅŸä Ÿàÿ∂ÿπ ÿπŸÑÿßŸÖÿ© ÿßŸÑŸÖŸÇÿ±Ÿàÿ°: ${error.message}`);
      throw error;
    }
  }

  /**
   * ÿ≠ÿ∞ŸÅ ÿßŸÑÿ•ÿ¥ÿπÿßÿ±
   */
  async deleteNotification(notificationId) {
    try {
      return await Notification.deleteOne({
        id: notificationId
      });
    } catch (error) {
      logger.error(`‚ùå ÿÆÿ∑ÿ£ ŸÅŸä ÿ≠ÿ∞ŸÅ ÿßŸÑÿ•ÿ¥ÿπÿßÿ±: ${error.message}`);
      throw error;
    }
  }

  /**
   * ÿ™ŸÇŸäŸäŸÖ ÿßŸÑÿ•ÿ¥ÿπÿßÿ±
   */
  async rateNotification(notificationId, rating, feedback = '') {
    try {
      return await Notification.updateOne({
        id: notificationId
      }, {
        rate: Math.min(5, Math.max(1, rating)),
        feedback,
        ratedAt: new Date()
      });
    } catch (error) {
      logger.error(`‚ùå ÿÆÿ∑ÿ£ ŸÅŸä ÿ™ŸÇŸäŸäŸÖ ÿßŸÑÿ•ÿ¥ÿπÿßÿ±: ${error.message}`);
      throw error;
    }
  }
}

