{
  "name": "intelligent-agent-backend",
  "version": "2.0.0",
  "description": "Intelligent Agent Backend with Advanced Features",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only backend/app.ts",
    "dev:ml": "ts-node-dev --respawn --transpile-only backend/ml-server.ts",
    "start": "node dist/app.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=\\.unit\\.test\\.ts",
    "test:integration": "jest --testPathPattern=\\.integration\\.test\\.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json}\"",
    "clean": "rm -rf dist",
    "start:prod": "NODE_ENV=production node dist/app.js",
    "start:staging": "NODE_ENV=staging node dist/app.js",
    "docker:build": "docker build -t intelligent-agent-backend .",
    "docker:run": "docker run -p 3000:3000 intelligent-agent-backend",
    "docker:stop": "docker stop $(docker ps -q --filter ancestor=intelligent-agent-backend)",
    "monitor:start": "pm2 start dist/app.js --name 'agent-backend'",
    "monitor:stop": "pm2 stop agent-backend",
    "monitor:logs": "pm2 logs agent-backend",
    "db:seed": "ts-node backend/scripts/seed-db.ts",
    "db:migrate": "ts-node backend/scripts/migrations.ts",
    "db:pool-config": "ts-node backend/scripts/db-pool-optimizer.ts recommend production",
    "cache:clear": "node backend/scripts/clear-cache.js all",
    "health:check": "node backend/scripts/health-check.js",
    "verify:deploy": "node backend/scripts/verify-deployment.js",
    "logs:export": "ts-node backend/scripts/export-logs.ts"
  },
  "keywords": [
    "ai",
    "ml",
    "graphql",
    "websocket",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "apollo-server-express": "^3.13.0",
    "graphql": "^16.8.1",
    "@graphql-tools/schema": "^10.0.2",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.3",
    "ws": "^8.16.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "bull": "^4.12.0",
    "ioredis": "^5.3.2",
    "express-rate-limit": "^7.1.5",
    "rate-limit-redis": "^4.2.0",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.14.0",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "mathjs": "^12.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/ws": "^8.5.10",
    "@types/nodemailer": "^6.4.14",
    "@types/pdfkit": "^0.13.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcrypt": "^5.0.2",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "prettier": "^3.2.4",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}