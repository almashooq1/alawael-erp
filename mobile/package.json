{
    "name": "saudi-license-mobile",
    "version": "2.1.0",
    "description": "نظام إدارة الرخص السعودية - تطبيق الهاتف الذكي",
    "main": "index.js",
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "build-android": "cd android && ./gradlew assembleRelease && cd ..",
        "build-ios": "cd ios && xcodebuild -workspace LicenseApp.xcworkspace -scheme LicenseApp -configuration Release && cd ..",
        "lint": "eslint .",
        "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
        "bundle-ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-native": "0.72.0",
        "@react-navigation/native": "^6.1.9",
        "@react-navigation/bottom-tabs": "^6.5.9",
        "@react-navigation/stack": "^6.3.19",
        "react-native-screens": "^3.26.0",
        "react-native-safe-area-context": "^4.7.2",
        "react-native-gesture-handler": "^2.14.0",
        "react-native-reanimated": "^3.5.0",
        "@react-native-async-storage/async-storage": "^1.21.0",
        "axios": "^1.6.0",
        "moment": "^2.29.4",
        "moment-hijri": "^2.3.1",
        "react-native-elements": "^3.4.0",
        "react-native-svg": "^13.14.0",
        "react-native-svg-charts": "^5.4.0",
        "react-native-camera": "^4.2.1",
        "react-native-document-picker": "^9.0.0",
        "react-native-file-viewer": "^2.2.0",
        "react-native-share": "^8.2.0",
        "react-native-push-notification": "^8.1.1",
        "react-native-biometrics": "^2.2.2",
        "react-native-fingerprint-scanner": "^10.1.0",
        "react-native-webview": "^13.6.3",
        "redux": "^4.2.1",
        "react-redux": "^8.1.3",
        "redux-thunk": "^2.4.2",
        "redux-persist": "^6.0.0",
        "formik": "^2.4.5",
        "yup": "^1.3.3",
        "@react-native-firebase/app": "^18.7.0",
        "@react-native-firebase/messaging": "^18.7.0",
        "@react-native-firebase/analytics": "^18.7.0",
        "lottie-react-native": "^6.4.0",
        "react-native-flash-message": "^0.1.24",
        "react-native-linear-gradient": "^2.8.3",
        "react-native-modal": "^13.0.0",
        "react-native-date-picker": "^4.2.12",
        "react-native-i18n": "^2.0.15"
    },
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@babel/preset-react": "^7.22.15",
        "@babel/preset-typescript": "^7.23.0",
        "@react-native-community/eslint-config": "^3.2.0",
        "babel-jest": "^29.7.0",
        "jest": "^29.7.0",
        "metro-react-native-babel-preset": "0.76.8",
        "typescript": "^5.2.2",
        "@types/react": "^18.2.37",
        "@types/react-native": "^0.72.2",
        "eslint": "^8.51.0",
        "prettier": "^3.0.3",
        "detox": "^20.14.11",
        "detox-cli": "^20.14.11"
    },
    "jest": {
        "preset": "react-native",
        "setupFilesAfterEnv": [
            "<rootDir>/tests/setup.js"
        ]
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    }
}