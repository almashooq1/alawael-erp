{
  "name": "alaweal-center-management-system",
  "version": "2.1.0",
  "description": "Alaweal Center - نظام ERP موحد شامل ومتكامل (معاد البناء)",
  "main": "backend/api/app.js",
  "scripts": {
    "start": "node run-server.js",
    "start:backend": "node run-server.js",
    "start:erp": "cd erp_new_system/backend && npm start",
    "dev:erp": "concurrently \"npm run dev:erp-backend\" \"npm run dev:erp-frontend\"",
    "dev:erp-backend": "cd erp_new_system/backend && nodemon server.js",
    "dev:erp-frontend": "cd erp_new_system/frontend && npm start",
    "build:erp-frontend": "cd erp_new_system/frontend && npm run build",
    "serve:erp-frontend": "cd erp_new_system/frontend && npx serve -s build -l 3002",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && nodemon server.js",
    "dev:frontend": "cd frontend/admin-dashboard && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend/admin-dashboard && npm run build",
    "build:backend": "node -e \"console.log('backend build skipped (no build step)')\"",
    "test": "jest -c backend/jest.config.js",
    "test:watch": "jest -c backend/jest.config.js --watch",
    "test:coverage": "jest -c backend/jest.config.js --coverage",
    "test:verbose": "jest -c backend/jest.config.js --verbose",
    "test:ci": "jest -c backend/jest.config.js --ci --coverage --maxWorkers=2",
    "prepare": "husky install",
    "test:tracking": "powershell -ExecutionPolicy Bypass -File ./scripts/testing/test-request-tracking.ps1",
    "test:full": "powershell -ExecutionPolicy Bypass -File ./scripts/testing/test-full-system.ps1",
    "lint": "eslint backend/api",
    "lint:fix": "eslint backend/api --fix",
    "lint:check": "eslint backend/api --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run lint:check && npm run format:check && npm run test:ci",
    "validate:env": "node scripts/validate-env.js",
    "prestart": "npm run validate:env",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "deploy:prod": "./scripts/deployment/deploy-production.sh",
    "deploy:rollback": "./scripts/deployment/rollback.sh",
    "pre-deploy": "./scripts/deployment/pre-deploy.sh",
    "post-deploy": "./scripts/deployment/post-deploy.sh",
    "package": "./scripts/deployment/package-for-deployment.sh",
    "package:win": "powershell -ExecutionPolicy Bypass -File ./scripts/deployment/package-for-deployment.ps1",
    "health:check": "node scripts/monitoring/health-check.js",
    "health:watch": "node scripts/monitoring/health-check.js --watch",
    "performance:monitor": "node scripts/monitoring/performance-monitor.js",
    "performance:watch": "node scripts/monitoring/performance-monitor.js --continuous",
    "performance:test": "npm test -- --coverage=false --maxWorkers=4",
    "security:audit": "npm audit --audit-level=moderate",
    "security:full": "npm audit && npm run lint:check",
    "db:migrate": "node -e \"console.log('Migration: Add your migration script')\"",
    "db:seed": "node -e \"console.log('Seeding: Add your seed script')\"",
    "cache:clear": "node -e \"console.log('Cache cleared successfully')\"",
    "analyze": "npm run lint && npm run test:coverage && npm run security:audit",
    "monitor:all": "concurrently \"npm run performance:watch\" \"npm run health:watch\""
  },
  "keywords": [
    "rehabilitation",
    "management",
    "system",
    "healthcare",
    "arabic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@microsoft/applicationinsights-react-native": "^4.3.8",
    "@notionhq/client": "5.8.0",
    "@pnp/sp": "4.17.0",
    "@types/graphql": "14.2.3",
    "@types/multer": "2.0.0",
    "@types/uuid": "10.0.0",
    "@types/ws": "8.18.1",
    "airtable": "0.12.2",
    "apollo-server-express": "3.13.0",
    "applicationinsights": "^3.12.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "box-node-sdk": "10.3.0",
    "compression": "1.8.1",
    "cors": "^2.8.5",
    "date-fns": "4.1.0",
    "dotenv": "^16.4.5",
    "dropbox": "10.34.0",
    "exceljs": "^4.4.0",
    "express": "4.22.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "7.3.1",
    "fastify": "^5.6.2",
    "graphql": "16.12.0",
    "helmet": "^7.1.0",
    "ioredis": "5.9.2",
    "isomorphic-dompurify": "^2.9.0",
    "joi": "^18.0.2",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "4.0.0",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.12",
    "paypal-rest-sdk": "^1.8.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "pino": "^8.20.0",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.1.3",
    "qrcode.react": "4.2.0",
    "razorpay": "^2.9.6",
    "recharts": "^3.6.0",
    "sequelize": "^6.37.7",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.8.3",
    "stripe": "^20.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "8.3.2",
    "validator": "^13.11.0",
    "winston": "^3.19.0",
    "ws": "8.19.0",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/adm-zip": "0.5.7",
    "@types/bcryptjs": "2.4.6",
    "@types/bunyan": "1.8.11",
    "@types/compression": "1.8.1",
    "@types/express": "4.17.14",
    "@types/jest": "30.0.0",
    "@types/jspdf": "1.3.3",
    "@types/mongoose": "5.11.96",
    "@types/mysql": "2.15.27",
    "@types/node-fetch": "2.6.13",
    "@types/node-schedule": "2.1.8",
    "@types/nodemailer": "7.0.5",
    "@types/pg": "8.16.0",
    "@types/supertest": "6.0.3",
    "@types/swagger-ui-express": "4.1.8",
    "@vitest/coverage-c8": "0.33.0",
    "@vitest/coverage-v8": "4.0.18",
    "babel-jest": "^30.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.0.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "29.7.0",
    "jest-coverage-badges": "1.1.2",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "mongodb-memory-server": "11.0.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "redis": "^4.6.13",
    "supertest": "^6.3.3",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,jsx,json,md}": [
      "prettier --write"
    ],
    "backend/api/**/*.js": [
      "eslint --fix --quiet --max-warnings=0"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
